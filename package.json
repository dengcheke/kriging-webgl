{
    "name": "kriging-webgl",
    "version": "1.0.1",
    "description": "kriging.js webgl accelerate version",
    "keywords": ["kriging", "webgl"],
    "type": "module",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        }
    },
    "files": ["dist"],
    "repository": {
        "type": "git",
        "url": "https://github.com/dengcheke/kriging-webgl.git"
    },
    "scripts": {
        "dev": "vite --config ./vite.example.ts",
        "build-example": "vite build --config ./vite.example.ts",
        "build": "vite build --config ./vite.build.ts && npm run build-dts",
        "build-dts": "tsc -p tsconfig.build.json"
    },
    "devDependencies": {
        "es-toolkit": "^1.39.9",
        "sass-embedded": "^1.90.0",
        "twgl.js": "^7.0.0",
        "typescript": "~5.8.3",
        "vite": "^7.0.4"
    },
    "dependencies": {
        "lru-cache": "^11.1.0"
    }
}